{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug",
            "program": "${workspaceFolder}/bazel-out/k8-dbg/bin/sample/test.sh.runfiles/__main__/rules/test_runner/test_runner",
            "args": [
                "-i",
                "sample/test.cc_static_test.info_binary",
                "-s",
                "sample/test.cc",
                "-c",
                "external/llvm_toolchain/bin/cc_wrapper.sh",
                "-j",
                "$JUNIT",
                "--",
                "-U_FORTIFY_SOURCE",
                "--target=x86_64-unknown-linux-gnu",
                "-U_FORTIFY_SOURCE",
                "-fstack-protector",
                "-fno-omit-frame-pointer",
                "-fcolor-diagnostics",
                "-Wall",
                "-Wthread-safety",
                "-Wself-assign",
                "-g",
                "-fstandalone-debug",
                "-std=c++17",
                "-stdlib=libc++",
                "-frandom-seed=dummy_output.o",
                "-iquote",
                ".",
                "-iquote",
                "bazel-out/k8-dbg/bin",
                "-Ilib/_virtual_includes/static_test",
                "-Ilib/private/_virtual_includes/support",
                "-Wall",
                "-Wpedantic",
                "-ferror-limit=4",
                "-g",
                "-std=c++17",
                "-no-canonical-prefixes",
                "-Wno-builtin-macro-redefined",
                "-D__DATE__=redacted",
                "-D__TIMESTAMP__=redacted",
                "-D__TIME__=redacted",
                "-fdebug-prefix-map=external/llvm_toolchain_llvm/=__bazel_toolchain_llvm_repo__/",
                "-c",
                "sample/test.cc",
                "-o",
                "dummy_output.o",
            ],
            "cwd": "${workspaceFolder}/bazel-out/k8-dbg/bin/sample/test.sh.runfiles/__main__",
            "sourceMap": {
                "/proc/self/cwd": "${workspaceFolder}"
            }
        }
    ]
}